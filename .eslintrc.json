{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "requestAnimationFrame": "readonly",
    "cancelAnimationFrame": "readonly"
  },
  "rules": {
    "max-lines": [
      "off"
    ],
    "linebreak-style": [
      "off"
    ],
    "semi": [
      "off"
    ],
    "no-unused-vars": [
      "off"
    ],
    "no-console": [
      "off"
    ],
    "no-constant-condition": [
      "off"
    ],
    "prefer-const": [
      "off"
    ],
    "prefer-arrow-callback": [
      "off"
    ],
    "arrow-spacing": [
      "off"
    ],
    "space-before-function-paren": [
      "off"
    ],
    "object-curly-spacing": [
      "off"
    ],
    "array-bracket-spacing": [
      "off"
    ],
    "keyword-spacing": [
      "off"
    ],
    "comma-spacing": [
      "off"
    ],
    "no-trailing-spaces": [
      "off"
    ],
    "eol-last": [
      "off"
    ],
    "max-len": [
      "off"
    ],
    "no-var": [
      "off"
    ],
    "eqeqeq": [
      "off"
    ],
    "curly": [
      "off"
    ],
    "brace-style": [
      "off"
    ]
  }
}
// eslint-disable-next-line eol-last